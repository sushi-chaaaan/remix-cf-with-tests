{
  "name": "resas-viewer",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "pnpm run /^lint:.*/",
    "lint:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --max-warnings 0 --fix .",
    "lint:stylelint": "stylelint --cache --cache-location ./node_modules/.cache/stylelint --fix **/*.{css,scss}",
    "fmt": "prettier --write .",
    "start": "wrangler pages dev ./build/client",
    "check": "pnpm run /^check:.*/",
    "check:tsc": "tsc",
    "check:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --max-warnings 0",
    "check:prettier": "prettier --check",
    "check:stylelint": "stylelint --cache --cache-location ./node_modules/.cache/stylelint",
    "typegen": "wrangler types",
    "preview": "pnpm run build && wrangler pages dev",
    "test": "vitest",
    "test:e2e": "pnpm exec playwright test",
    "test:ct": "playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.9.2",
    "@remix-run/cloudflare-pages": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "clsx": "^2.1.1",
    "isbot": "^4.4.0",
    "miniflare": "^3.20240512.0",
    "react": "19.0.0-rc-915b914b3a-20240515",
    "react-dom": "19.0.0-rc-915b914b3a-20240515",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1"
  },
  "overrides": {
    "react": "19.0.0-rc-915b914b3a-20240515",
    "react-dom": "19.0.0-rc-915b914b3a-20240515"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@double-great/stylelint-a11y": "^3.0.2",
    "@playwright/experimental-ct-react": "^1.44.0",
    "@playwright/test": "^1.44.0",
    "@remix-run/dev": "^2.9.2",
    "@remix-run/testing": "^2.9.2",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@virtual-live-lab/eslint-config": "^2.0.1",
    "@virtual-live-lab/prettier-config": "^2.0.2",
    "@virtual-live-lab/stylelint-config": "^2.0.2",
    "@virtual-live-lab/tsconfig": "^2.0.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.2.0",
    "happy-dom": "^14.11.0",
    "lefthook": "^1.6.12",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "wrangler": "^3.57.0"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "packageManager": "pnpm@9.1.1"
}
